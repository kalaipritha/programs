#include <iostream>

using namespace std;

int main()
{
   int n,i;
   
   cout<<"enter the number of elements";
   cin>>n;
   int a[n];
   cout<<"\nenter the array elements :";
   for(i=0;i<n;i++)
   {
       cin>>a[i];
   }
   int m;
   cout<<"\nenter the value of m";
   cin>>m;
   for(i=0;i<m;i++)
   {
       if(a[i]==i)
       {
           continue;
       }
       else
       {
           cout<<"\n the first smallest missing element is "<<i;
           break;
       }
   }
   
   return 0;
}
