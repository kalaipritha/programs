#include <iostream>
using namespace std;
int main()
{
     int a[10];
   cout<<"enter an array";
   for(int i=0;i<8;i++)
   {
       cin>>a[i];
   }
   int r=7;
   for(int i=0;i<r;i++)
   {
       if(a[i]==0)
       {
           for(int k=i;k<7;k++)
           a[k]=a[k+1];
           a[7]=0;
       }
   }
   cout<<"the array is ";
   for(int i=0;i<8;i++)
   cout<<a[i];

    return 0;
}

