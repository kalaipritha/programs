#include <iostream>
using namespace std;
void print(int a[],int n)
{
    cout<<"\n";
    for(int i=0;i<n;i++)
   {
      cout<<a[i]<<" ";  
   }
}
int main()
{
   int n,i;
   cout<<"enter the number of elements";
   cin>>n;
   int a[n];
   cout<<"enter the array elements :";
   for(i=0;i<n;i++)
   {
       cin>>a[i];
   }
   for(i=0;i<n;i++)
   {
       cout<<"\nthe value of i is"<<i;
       int temp=a[n-1],p=n-2;
       while(p>=i)
       {
           a[p+1]=a[p];
           p--;
       }
       a[i]=temp;
       i++;
   }
   print(a,n);
    return 0;
}
